}

/*选填  */

.selection {
    width: 100%;
    background-color: #fff;
    margin-top: 18rpx;
}

.selection .selection-title {
    width: 100%;
    height: 84rpx;
    display: flex;
    align-items: center;
    padding: 0 24rpx;
    box-sizing: border-box;
}

.selection .selection-title image {
    width: 32rpx;
    height: 32rpx;
    vertical-align: middle;
    margin-right: 11rpx;
}

.selection .selection-title text {
    font-size: 26rpx;
    color: #282828;
}

.selection .selection-title text text {
    font-size: 22rpx;
}

.selection .selection-C {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 9rpx 24rpx 30rpx;
    box-sizing: border-box;
    border-top: 2rpx solid #eef1f4;
}
.selection .selection-C button {
    width: 160rpx;
    height: 58rpx;
    font-size: 24rpx;
    color: #282828;
    text-align: center;
    line-height: 58rpx;
    background-color: #eeeef4;
    border: 1rpx solid #eeeef4;
    border-radius: 4rpx;
    margin-top: 21rpx;
    position: relative;
}

.selection .selection-C .selection-item image {
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    right: 0;
    bottom: 0;
}

.selection .selection-C .fill {
    width: 355rpx;
    height: 58rpx;
}

.selection-item.active {
    border: 1rpx solid #5e94f5;
    background-color: #fff !important;
    color: #5e94f5 !important;
}

