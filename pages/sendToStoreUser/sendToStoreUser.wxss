/*添加图片  */

.imgs {
    width: 100%;
    background-color: #fff;
    padding: 0 24rpx;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin-top: 2rpx;
}

.imgs .img-item {
    width: 210rpx;
    height: 210rpx;
    margin-top: 29rpx;
    margin-bottom: 29rpx;
}

.imgs .img-item .default {
    width: 100%;
    height: 100%;
    box-shadow: 0px 3px 14px 0px rgba(98, 126, 167, 0.2);
    border-radius: 8px;
}

.imgs .img-item .default image {
    width: 100%;
    height: 100%;
}

.imgs .img-item .pic {
    width: 100%;
