page {
  background-color: #eef1f4;
  font-size: 24rpx;
}
.top {
  border-bottom: 2rpx solid #eef1f4;
  padding: 24rpx;
  background: #fff;
}
.info_all {
  background: #fff;
  display: flex;
  align-items: center;
  padding: 24rpx;
  margin-bottom: 22rpx;
  justify-content: space-between;
}
.info_all_left {
  flex: 1;
  display: flex;
}
.portrait {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  overflow: hidden;
  margin-right: 30rpx;
}
.name {
  font-weight: 900;
  margin-bottom: 20rpx;
}
.tab {
  position: relative;
  border-bottom: 2rpx solid #eef1f4;
}
.tab_main {
  display: flex;
  background: #fff;
  height: 100rpx;
  justify-content: space-between;
  align-items: center;
  padding: 0 135rpx;
}
.tab_item {
  width: 100rpx;
  line-height: 100rpx;
  height: 100rpx;
  text-align: center;
}
.tab .line {
  position: absolute;
  width: 100rpx;
  height: 10rpx;
  background: #5e95f4;
  border-radius: 10rpx;
  bottom: 0rpx;
  transition: 0.5s;
}
.tab_item.active {
  color: #5e95f4;
}

/* 清洗 */
.wash .list {
  background: #fff;
}
.wash_btnBox {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 20rpx;
}
.wash .add_btn {
  width: 250rpx;
	height: 70rpx;
  background: #fff;
  line-height: 70rpx;
  color: #5e95f4;
  text-align: center;
  border-radius: 6rpx;
}
.wash .wash_btnBox .empty {
  width: 250rpx;
	height: 70rpx;
  background: #fff;
  line-height: 70rpx;
  color:red;
  text-align: center;
  border-radius: 6rpx;
}
.wash .add_btn image {
  width: 38rpx;
	height: 38rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}
.list .item {
  display: flex;
  padding: 27rpx 24rpx;
}
.list .goods_img {
  width: 140rpx;
	height: 140rpx;
  margin-right: 10rpx;
  border-radius: 6rpx;
}
.cz {
  padding: 20rpx 46rpx;
  text-align: center;
  background-color: #f6f5fa;
	border-radius: 32rpx;
  margin: 0 10rpx;
}
.cz.active {
  background-color: #e7f3ff;
  color: #5e95f4;
}
.PZ,.FMP,.QT {
  display: flex;
  height: 100%;
  justify-content: space-between;
  align-items: center;
}
.item .move {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.item .move image {
  width: 12rpx;
	height: 22rpx;
  margin-left: 12rpx;
}
/*附加服务  */
.addService {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9;
}

.addService .box {
    width: 670rpx;
    padding: 0 40rpx 40rpx;
    box-sizing: border-box;
    background-color: #eef1f4;
    margin: 30% auto;
}

.addService .box .title {
    width: 100%;
    text-align: center;
    font-size: 26rpx;
    color: #282828;
    line-height: 26rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}

.addService .box .content {
    width: 590rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.addService .box .content image {
    width: 22rpx;
    height: 22rpx;
    margin-right: 12rpx;
}

.addService .box .content .item {
    width: 100%;
    height: 170rpx;
    padding: 0 45rpx;
    box-sizing: border-box;
    border-bottom: 4rpx solid #eef1f4;
}

.addService .box .content .item .top {
    font-size: 26rpx;
    color: #282828;
    line-height: 26rpx;
    padding-top: 30rpx;
    box-sizing: border-box;
}

.addService .box .content .item .bottom {
    margin-top: 30rpx;
    display: flex;
    justify-content: flex-start;
}

.addService .box .content .item .bottom view text {
    padding: 17rpx 40rpx;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #282828;
    background-color: #f5f6fa;
    border-radius: 28rpx;
    margin-right: 26rpx;
}

.addService .box .prompt {
    font-size: 20rpx;
    line-height: 20rpx;
    color: #5f6266;
    margin-top: 22rpx;
}

.addService .box .button {
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.addService .box .button .sure {
    width: 150rpx;
    height: 56rpx;
    position: relative;
}

.addService .box .button .sure image {
    width: 100%;
    height: 100%;
}

.addService .box .button {
    margin-top: 30rpx;
}

.addService .box .button .sure text {
    font-size: 28rpx;
    color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -28rpx;
    margin-top: -19rpx;
}

.addService .box .button .cancel {
    width: 150rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
    font-size: 28rpx;
    color: #fff;
    background-color: #d9dce2;
    border-radius: 28rpx;
}
.addActive {
    background-color: #e7f3ff !important;
    color: #5e95f4 !important;
}
/* 清洗的提交按钮 */
.wash .submit {
    width: 632rpx;
    height: 74rpx;
    position: relative;
    margin: 38rpx auto;
    box-sizing: border-box;
}

.wash .submit image {
    width: 100%;
    height: 74rpx;
    display: inline-block;
}

.wash .submit .xiadan {
    width: 100%;
    height: 74rpx;
    line-height: 74rpx;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    position: absolute;
    left: 0;
    top: 0;
}

.wash .submit .xiadan text {
    margin: 0 13rpx;
}

.wash .submit .eight {
    width: 86rpx;
    height: 34rpx;
    position: absolute;
    right: -20rpx;
    top: -15rpx;
}
.wash .select {
  position: relative
}
.wash .remove {
  position: absolute;
  width: 25rpx;
  height: 25rpx;
  top: -36rpx;
  right: 0rpx;
}
.wash .remove image {
  display: inline-block;
  width: 100%;
  height: 100%;
}



































/* 修复 */

