/* page/cleanOrder/cleanOrder.wxss */

page {
  background-color: #eef1f4;
}

/*邮寄地址  */

.mail {
  width: 100%;
  height: 173rpx;
  position: relative;
}

.mail .bg {
  width: 100%;
  height: 100%;
}

.mail .C {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
  padding-right: 24rpx;
  box-sizing: border-box;
}

.user {
  width: 100%;
  height: 173rpx;
  padding-top: 19rpx;
  box-sizing: border-box;
}

.user .userInfo {
  padding-left: 24rpx;
  box-sizing: border-box;
  margin-bottom: 10rpx;
}

.user .userInfo image {
  width: 22rpx;
  height: 26rpx;
  margin-right: 17rpx;
  vertical-align: middle;
}

.user .userInfo text {
  font-size: 28rpx;
  margin-right: 42rpx;
}

.user .address {
  padding-left: 64rpx;
  padding-right: 42rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  line-height: 40rpx;
}

.mail .right {
  width: 14rpx;
  height: 25rpx;
}

.mail .right image {
  width: 100%;
  height: 100%;
}

/*填写邮寄信息  */

.no-mail {
  width: 100%;
  height: 128rpx;
  position: relative;
}

.no-mail .bg {
  width: 100%;
  height: 100%;
}

.no-mail .C {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.no-mail .C .address {
  display: flex;
  align-items: center;
}

.no-mail .C .address image {
  width: 34rpx;
  height: 38rpx;
  margin-right: 23rpx;
}

.no-mail .C .address text {
  font-size: 36rpx;
  color: #fff;
}

.no-mail .C .right image {
  width: 15rpx;
  height: 27rpx;
}

/*提示  */

.prompt {
  width: 100%;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: rgb(253, 250, 233);
  font-size: 22rpx;
  color: rgb(152, 152, 152);
}

/*上传图片部分  */

.upload {
  width: 100%;
  padding: 22rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  background-color: #fff;
  margin-bottom: 18rpx;
}

.upload .U-left {
  width: 126rpx;
  height: 126rpx;
  border-radius: 6rpx;
}

.upload .U-left image {
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
}

.upload .U-right {
  width: 548rpx;
  margin-left: 22rpx;
}

.upload .U-right .U-price {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}

.upload .U-right .U-price text:nth-child(1) {
  font-size: 26rpx;
  color: rgb(40, 40, 40);
}

.upload .U-right .U-price text:nth-child(2) {
  font-size: 30rpx;
  color: rgb(40, 40, 40);
}

.upload .U-right .U-fuwu {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  /* justify-content: flex-start; *//* justify-content: space-between; */
  margin-top: 15rpx;
}

.upload .U-right .U-fuwu text {
  padding: 3rpx 6rpx;
  box-sizing: border-box;
  border: 1px solid #5e94f5;
  border-radius: 6rpx;
  font-size: 21rpx;
  color: #5e94f5;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
}

