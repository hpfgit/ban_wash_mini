page {
    background-color: #eef1f4;
}

.page {
    width: 100%;
    height: 100%;
}

.all {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.all .item {
    width: 100%;
    height: 170rpx;
    background-color: #fff;
    border-radius: 8rpx;
    margin-top: 18rpx;
    padding: 32rpx 15rpx 32rpx 40rpx;
    box-sizing: border-box;
    display: flex;
    position: relative;
}

.all .item .textImg {
    width: 73rpx;
    height: 73rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.all .item .left {
    display: flex;
    align-items: center;
    border-right: 2rpx solid #f5f4f4;
    margin-right: 30rpx;
}

.all .item .left image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 30rpx;
}

.all .item .middle view:nth-child(1) {
    font-size: 26rpx;
    color: #323741;
    line-height: 26rpx;
    margin-bottom: 19rpx;
}

.all .item .middle view:nth-child(2) {
    margin-bottom: 15rpx;
}

.all .item .middle view {
    width: 420rpx;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #858585;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.all .item .right {
    display: flex;
    align-items: center;
    margin-left: 25rpx;
}

.all .item .right image {
    width: 80rpx;
    height: 80rpx;
}

/*添加新地址  */

.add {
    width: 304rpx;
    height: 80rpx;
    background-color: #5e95f4;
    border-radius: 40rpx;
    position: fixed;
    bottom: 70rpx;
    left: 50%;
    margin-left: -152rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.add image {
    width: 28rpx;
    height: 28rpx;
    margin-right: 29rpx;
}

.add text {
    font-size: 26rpx;
    color: #fff;
}

/*添加新地址模态框  */

.addModule {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.4);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9;
    padding: 100rpx 40rpx;
    box-sizing: border-box;
}

.addModule .new {
    width: 100%;
    height: 100%;
    background-color: #fff;
    position: relative;
    padding: 166rpx 66rpx 0;
    box-sizing: border-box;
    border-radius: 10rpx;
}

.addModule .new .close {
    width: 90rpx;
    height: 90rpx;
    position: absolute;
    left: 25rpx;
    top: 25rpx;
}

.addModule .new .del {
    width: 90rpx;
    height: 90rpx;
    position: absolute;
    right: 25rpx;
    top: 25rpx;
}

.addModule .new .common {
    width: 100%;
    height: 107rpx;
}

.addModule .new .common .title {
    width: 100%;
    font-size: 22rpx;
    line-height: 22rpx;
    color: #858585;
}

.addModule .new .common .input {
    border-bottom: 2rpx solid #e6e6e6;
    font-size: 26rpx;
    line-height: 26rpx;
    color: #282828;
    margin-top: 20rpx;
}

.addModule .new .diqu {
    margin-top: 30rpx;
}

.addModule .new .diqu .input {
    padding-bottom: 12rpx;
    margin-top: 30rpx;
}

.addModule .new .phone {
    margin-top: 30rpx;
}

.addModule .new .address {
    margin-top: 30rpx;
}

.addModule .new .default {
    font-size: 22rpx;
    color: #858585;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.wx-switch-input {
    width: 64rpx !important;
    height: 24rpx !important;
}

.wx-switch-input::before {
    width: 64rpx !important;
    height: 24rpx !important;
}

.wx-switch-input::after {
    width: 38rpx !important;
    height: 38rpx !important;
    margin-top: -8rpx;
}

.addModule .new .confirmAdd {
    width: 100%;
    height: 98rpx;
    position: absolute;
    bottom: 0;
    left: 0;
}

.addModule .new .confirmAdd image {
    width: 100%;
    height: 100%;
}

.addModule .new .confirmAdd text {
    font-size: 28rpx;
    color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -19rpx;
    margin-left: -56rpx;
}

/*选择地区弹出  */

.picker-view {
    width: 100%;
    display: flex;
    z-index: 12;
    background-color: #fff;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    height: 40vh;
}

.picker-item {
    line-height: 70rpx;
    margin-left: 5rpx;
    margin-right: 5rpx;
    text-align: center;
}

/*我也不知道是啥  */

.dialog-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 8;
    background: rgba(0, 0, 0, 0.3);
}

/*暂无收货地址  */

.noAddress {
    width: 100%;
    text-align: center;
    margin-top: 20rpx;
    font-size: 30rpx;
    font-weight: 600;
    color: #282828;
}
