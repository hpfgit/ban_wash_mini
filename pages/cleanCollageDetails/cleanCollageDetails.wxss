.shoes .shoes-C .C-left image {
    width: 100%;
    height: 100%;
}

.shoes .shoes-C .C-right {
    width: 549rpx;
}

.shoes .shoes-C .C-right .C-price {
    width: 100%;
    display: flex;
    justify-content: space-between;
    font-size: 26rpx;
    color: rgb(40, 40, 40);
    margin-top: 10rpx;
}

.shoes .shoes-C .C-right .C-fuwu {
    margin-top: 39rpx;
    display: flex;
    /* flex-wrap: wrap; */
}

.shoes .shoes-C .C-right .C-fuwu .type1 view {
    padding: 4rpx 7rpx;
    text-align: center;
