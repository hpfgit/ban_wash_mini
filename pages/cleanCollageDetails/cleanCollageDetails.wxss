    height: 70rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #eef1f4;
    position: relative;
    padding: 0 24rpx;
    box-sizing: border-box;
}

.shoes .shoes-title .title-left {
    font-size: 28rpx;
