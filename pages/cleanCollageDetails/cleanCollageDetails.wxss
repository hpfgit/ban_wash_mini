    padding: 0 24rpx;
}
.shoes .shoes-C .supplement {
    display: flex;
    font-size: 25rpx;
    height: 85rpx;
    justify-content: space-between;
    align-items: center;
    border-bottom: 2rpx solid #f2f2f2;
    padding: 0 24rpx;
}

.shoes .shoes-C .supplement .supplementText {
   
}
.shoes .shoes-C .supplement .supplementText image {
    margin-left: 22rpx;
}
.drop_down {
    width: 26rpx;
    height: 16rpx;
    color: #b2b2b2;
}
.fuwuInfo {
    font-size: 25rpx;
    padding: 30rpx 24rpx 0;
}
.fuwuInfoTop {
    display: flex;
    justify-content: space-between;
}
.fuwuInfo .fuwuInfo-left {
    display: flex;
}
.shoes .shoes-C .C-left {
    width: 126rpx;
    height: 126rpx;
    margin-right: 22rpx;
}

.fuwuInfo-list {

}
.fuwuInfo-list .fwItem-box {
    display: flex;
}
.fuwuInfo-list .fuwuInfo-item {
    padding: 30rpx 0;
