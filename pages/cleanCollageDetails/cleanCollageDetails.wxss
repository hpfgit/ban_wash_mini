}

.shoes .shoes-B {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    display: flex;
    justify-content: space-between;
}

/*服务介绍  */

.intro {
    width: 100%;
    margin-top: 18rpx;
    background-color: #fff;
    padding: 0 25rpx;
    box-sizing: border-box;
}

.intro .intro-item {
    padding: 30rpx 0;
    border-bottom: 1rpx solid rgb(242, 242, 242);
    font-size: 26rpx;
    display: flex;
    justify-content: flex-start;
}

.intro .intro-item .father {
    width: 25%;
    font-weight: 600;
}

.intro .intro-item .child {
    width: 100%;
}

.intro .intro-item .child text {
    font-size: 26rpx;
    font-weight: 400;
    margin-right: 14rpx;
}

/*订单信息  */

.order {
    width: 100%;
    margin-top: 18rpx;
    padding: 0 25rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.order .order-top {
    padding: 30rpx 0;
    box-sizing: border-box;
    font-size: 26rpx;
    color: rgb(95, 98, 102);
    border-bottom: 1rpx solid rgb(238, 238, 238);
}

.order .order-top view:first-child {
    margin-bottom: 23rpx;
}

.order .order-top view text {
    width: 66rpx;
    height: 30rpx;
    font-size: 24rpx;
    border: 1px solid rgb(152, 152, 152);
    border-radius: 6rpx;
    margin-left: 40rpx;
}

.order .order-bottom {
    padding: 30rpx 0;
    box-sizing: border-box;
    font-size: 26rpx;
    color: rgb(95, 98, 102);
}

.order .order-bottom view:first-child {
    margin-bottom: 23rpx;
}

/*付款金额  */

.pay {
    width: 100%;
    margin-top: 18rpx;
    padding: 0rpx 25rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.pay .pay-item {
    width: 100%;
    height: 85rpx;
    font-size: 26rpx;
    color: rgb(40, 40, 40);
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.pay .discount view:last-child {
    color: #5e94f5;
}

.pay .real {
    font-size: 28rpx;
    font-weight: 600;
    border-top: 1rpx solid #eef1f4;
}


